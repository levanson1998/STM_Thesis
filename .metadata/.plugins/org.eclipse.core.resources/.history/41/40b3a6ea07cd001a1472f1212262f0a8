float PID_out_max, PID_out_min;

void PID_Init(float *_Kp, float *_Ki, float *_Kd, float _T) {
	for (int i = 0; i < NoPID; i++) {
		PID_Kp[i] = Kp[i];
		PID_Ki[i] = Ki[i];
		PID_Kd[i] = Kd[i];
	}
	PID_T = T;
}



